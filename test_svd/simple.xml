								<enumeratedValue>
									<name>Underflow</name>
									<description>A minimum of one underflow occured since last clear</description>
									<value>1</value>
								</enumeratedValue>
							</enumeratedValues>
						</field>
						<!-- OV: Shows if an overflow occured -->
